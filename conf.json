{
    "logfile": "./logs/log",
    "gps": {
        "port": "/dev/ttyUSB0",
        "baud": 38400
    },
    "pathfinding":{
        "port": "/dev/ttyACM0",
        "baud": 9600,
        "safe_length": 10,
        "ray_maximum": 100,
        "perform_growth" : true,
        "add_degree_coefficient": true
    },
    "nodelist": {
        "file":"./nodes"
    },
    "motorcontroller": {
       "port": "/dev/arduino_motorcontroller",
        "baud": 57600,
        "gillens_equation":{
            "gillens_limit": 16000,
            "turn_coefficient": 1.0
        }
        
    },
    "start_waiter": {
        "port": "/dev/arduino_logger",
        "baud": 9600
    }
}
